{
  "name": "@graphql-codegen/java-common",
  "version": "2.2.6",
  "type": "module",
  "description": "GraphQL Code Generator utils library for developing Java plugins",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-code-generator-community.git",
    "directory": "packages/plugins/java/common"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "lint": "eslint **/*.ts"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^3.0.0",
    "@graphql-codegen/visitor-plugin-common": "2.13.8",
    "auto-bind": "~4.0.0",
    "min-indent": "1.0.1",
    "tslib": "~2.4.0",
    "unixify": "^1.0.0"
  },
  "devDependencies": {
    "java-ast": "0.3.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
