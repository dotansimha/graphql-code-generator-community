{
  "name": "@graphql-codegen/c-sharp",
  "version": "5.1.0",
  "type": "module",
  "description": "GraphQL Code Generator plugin for generating CSharp code based on a GraphQL schema",
  "repository": "git@github.com:dotansimha/graphql-code-generator.git",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "scripts": {
    "lint": "eslint **/*.ts",
    "test": "jest --no-watchman"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@graphql-codegen/c-sharp-common": "1.1.0",
    "@graphql-codegen/plugin-helpers": "^5.0.4",
    "@graphql-codegen/visitor-plugin-common": "^5.3.1",
    "change-case-all": "1.0.15",
    "tslib": "~2.6.0",
    "unixify": "^1.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.18.0"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "jest-junit": {
    "outputDirectory": "../../../../test-results/c-sharp"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
